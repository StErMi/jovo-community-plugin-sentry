{
  "name": "jovo-community-plugin-sentry",
  "version": "1.1.0",
  "description": "A Jovo Framework plugin for Sentry",
  "main": "dist/src/index",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "tsc": "tsc",
    "rimraf": "rimraf ./dist",
    "typedoc": "typedoc --out ./docs ./ --mode file",
    "test": "jest"
  },
  "author": "Mark Tucker",
  "license": "MIT",
  "homepage": "https://github.com/jovo-community/jovo-community-plugin-sentry#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jovo-community/jovo-community-plugin-sentry"
  },
  "bugs": {
    "url": "https://github.com/jovo-community/jovo-community-plugin-sentry/issues"
  },
  "dependencies": {
    "jovo-core": "^2.2.10",
    "@sentry/node": "^5.15.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^10.3.1",
    "jest": "^24.8.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.8",
    "ts-jest": "^24.0.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "files": [
    "dist/src/**/*"
  ]
}
